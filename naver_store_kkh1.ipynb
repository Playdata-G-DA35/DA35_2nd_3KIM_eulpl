    "def extract_info(browser):\n",
    "    names = []\n",
    "    addresses = []\n",
    "\n",
    "    try:\n",
    "        # iframe으로 전환\n",
    "        iframe = browser.find_element(By.CSS_SELECTOR, \"iframe#mainFrame\")\n",
    "        browser.switch_to.frame(iframe)\n",
    "        \n",
    "        # 지도 이름 추출\n",
    "        name_element = browser.find_element(By.CSS_SELECTOR, \"strong.se-map-title\")\n",
    "        name_text = name_element.text\n",
    "        names.append(name_text)\n",
    "        \n",
    "        # 위치 정보 추출\n",
    "        address_element = browser.find_element(By.CSS_SELECTOR, \"#SE-ddce372e-1590-4f60-ae20-fc6e6e91c128 > div > div > div.se-module.se-module-map-text > a > p\")\n",
    "        address_text = address_element.text\n",
    "        addresses.append(address_text)\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"An error occurred: {e}\")\n",
    "    \n",
    "    return names, addresses\n",
    "\n",
    "names, addresses = extract_info(naver_driver)\n",
    "\n",
    "print(f\"Name: {names}\")\n",
    "print(f\"Address: {addresses}\")\n",
    "\n",
    "naver_driver.quit()"
